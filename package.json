{
  "name": "pankkiyhteys",
  "version": "1.2.0",
  "description": "",
  "keywords": [],
  "main": "index.js",
  "typings": "dist/types/pankkiyhteys.d.ts",
  "files": [
    "dist"
  ],
  "author": "Santeri Hurnanen <santeri@oikeuttaelaimille.fi>",
  "repository": {
    "type": "git",
    "url": "https://github.com/hyrsky/pankkiyhteys"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.10.0"
  },
  "scripts": {
    "lint": "eslint 'src/**/*.{js,json,ts,tsx}'",
    "prebuild": "rimraf dist",
    "build": "tsc --module commonJS && npm run docs",
    "docs": "typedoc --plugin typedoc-plugin-missing-exports --out docs --theme default src/pankkiyhteys.ts",
    "test": "jest --coverage",
    "debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "commit": "git-cz",
    "semantic-release": "semantic-release",
    "prepare": "husky install"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/test/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverageFrom": [
      "src/*.{js,ts}"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "{src,test}/**/*.ts": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      "@semantic-release/git"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.12",
    "@types/node-forge": "^1.3.11",
    "@types/request-promise-native": "^1.0.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "@typescript-eslint/parser": "^7.10.0",
    "commitizen": "^4.3.0",
    "coveralls": "^3.1.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-config": "^29.7.0",
    "lint-staged": "^15.2.4",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.7",
    "semantic-release": "^23.1.1",
    "source-map-support": "^0.5.21",
    "ts-jest": "^29.1.3",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typedoc-plugin-missing-exports": "^2.2.0",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@xmldom/xmldom": "^0.8.10",
    "debug": "^4.3.4",
    "fast-xml-parser": "^4.4.0",
    "node-forge": "^1.3.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "uuid": "^9.0.1",
    "xml-crypto": "^6.0.0",
    "xmlbuilder": "^15.1.1",
    "xpath": "^0.0.34"
  }
}
